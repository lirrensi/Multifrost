# Multifrost Environment Variables
# Copy this file to .env and fill in values as needed

# =============================================================================
# Optional: Override default ZeroMQ port range
# =============================================================================
# MULTIFROST_PORT_MIN=5000
# MULTIFROST_PORT_MAX=6000

# =============================================================================
# Optional: Service registry location (for connect mode)
# =============================================================================
# MULTIFROST_REGISTRY_DIR=~/.multifrost

# =============================================================================
# Optional: Logging configuration
# =============================================================================
# MULTIFROST_LOG_LEVEL=info
# MULTIFROST_LOG_FORMAT=json

# =============================================================================
# Development only (do not use in production)
# =============================================================================
# DEBUG_MULTIFROST=true
